        {
            "name": "UTM10",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": ["..\\..\\..\\..\\data\\surrey","..\\..\\..\\..\\data\\surrey\\Out_gdb.gdb", "UTM_10_featuredataset"]
        },
        {
            "name": "batch_clip",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": ["..\\..\\..\\..\\data\\BatchClipData\\TargetData", "..\\..\\..\\..\\data\\BatchClipData\\Sites", "..\\..\\..\\..\\data\\BatchClipData\\Output"]
        }
    ]
